{
  "name": "cash-app-clone",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@auth0/auth0-react": "1.3.0",
    "@emotion/react": "11.1.5",
    "@emotion/styled": "11.1.5",
    "@hookstate/core": "^3.0.8",
    "@hookstate/devtools": "^3.0.1",
    "@material-ui/core": "4.11.3",
    "@material-ui/icons": "4.11.2",
    "@material-ui/lab": "4.0.0-alpha.57",
    "@testing-library/jest-dom": "5.11.4",
    "@testing-library/react": "11.2.6",
    "@testing-library/user-event": "12.1.10",
    "@types/bcryptjs": "2.4.2",
    "@types/jest": "26.0.21",
    "@types/material-ui": "0.21.8",
    "@types/node": "14.14.35",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.2",
    "@types/styled-components": "5.1.9",
    "@xstate/react": "1.3.1",
    "autoprefixer": "10.2.6",
    "axios": "0.21.1",
    "bcryptjs": "2.4.3",
    "clsx": "1.1.1",
    "cors": "2.8.5",
    "cypress": "7.0.0",
    "faker": "^5.5.3",
    "formik": "2.2.6",
    "framer-motion": "3.10.5",
    "fuse.js": "6.4.6",
    "history": "5.0.0",
    "jest-dom": "4.0.0",
    "lowdb": "1.0.0",
    "ncp": "2.0.0",
    "passport": "0.4.1",
    "passport-local": "1.0.0",
    "postcss": "7",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-icons": "4.2.0",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "styled-components": "5.2.3",
    "typescript": "4.2.3",
    "web-vitals": "1.0.1",
    "yup": "0.32.9"
  },
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development concurrently yarn:start:react yarn:start:api:watch",
    "start:api": "yarn tsnode --files backend/app.ts",
    "start:api:watch": "nodemon --exec yarn tsnode --watch 'backend' backend/app.ts",
    "start:react": "react-scripts start",
    "db:seed": "yarn tsnode scripts/generateSeedData",
    "db:seed:dev": "ncp ./data/database-seed.json ./data/database.json",
    "db:seed:empty": "ncp ./data/empty-seed.json ./data/database.json",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "tsnode": "ts-node -P tsconfig.tsnode.json",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "no-unused-expressions": 0
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/cypress": "7.0.5",
    "@types/body-parser": "1.19.0",
    "@types/cors": "2.8.10",
    "@types/express": "4.17.11",
    "@types/express-paginate": "1.0.0",
    "@types/express-serve-static-core": "4.17.19",
    "@types/express-session": "1.17.3",
    "@types/lodash": "4.14.168",
    "@types/lowdb": "1.0.9",
    "@types/morgan": "1.9.2",
    "@types/passport": "1.0.6",
    "@types/react-router": "5.1.12",
    "@types/react-router-dom": "5.1.7",
    "@types/shortid": "0.0.29",
    "@types/uuid": "8.3.0",
    "@typescript-eslint/parser": "4.18.0",
    "babel-eslint": "10.1.0",
    "concurrently": "6.0.0",
    "cross-env": "7.0.3",
    "dotenv": "8.2.0",
    "eslint": "7.22.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "express-session": "1.17.1",
    "express-validator": "6.10.0",
    "husky": "^6.0.0",
    "morgan": "1.10.0",
    "nodemon": "2.0.7",
    "prettier": "2.2.1",
    "prettier-eslint": "12.0.0"
  }
}
